[project]
	description = Test Repo for gerrithub
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/mikehstrauss
[access "refs/for/refs/*"]
	read = group user/mikehstrauss
	push = group user/mikehstrauss
	label-Code-Review = -2..+2 group user/mikehstrauss
	label-Verified = -1..+1 group user/mikehstrauss
	rebase = group user/mikehstrauss
	removeReviewer = group user/mikehstrauss
	submit = group user/mikehstrauss
[access "refs/heads/*"]
	read = group user/mikehstrauss
	create = group user/mikehstrauss
	push = +force group user/mikehstrauss
	pushMerge = group user/mikehstrauss
[access "refs/tags/*"]
	push = +force group user/mikehstrauss
	pushTag = group user/mikehstrauss
	pushSignedTag = group user/mikehstrauss
